      <script src="../bower_components/html5shiv/dist/html5shiv.js"></script>
      <script src="../bower_components/respond/dest/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <a href="../" class="navbar-brand">EventPage</a>
          <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
          </button>
        </div>
        <div class="navbar-collapse collapse" id="navbar-main">
          <ul class="nav navbar-nav">
            <li>
              <a href="./mypage">MyPage</a>
            </li>
            <li>
              <a href="../planning">Event Planning</a>
            </li>
          </ul>

          <ul class="nav navbar-nav navbar-right">
            <li><a href="" target="_blank">Sign In</a></li>
            <li><a href="" target="_blank">Sign Up</a></li>
            <li><a href="" target="_blank">Sign Out</a></li>
          </ul>

        </div>
      </div>
    </div>

    <div class="container">

      <div class="page-header" id="banner">
        <div class="row">
          <div class="col-lg-12">
            <div class="page-header">
              <h1 id="forms">Forms</h1>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-lg-12">
            <form class="form-horizontal" action="/register" method="post">
              <fieldset>
                <div class="form-group">
                  <label for="eventName" class="col-lg-2 control-label">Event Name</label>
                  <div class="col-lg-10">
                    <input type="text" class="form-control" id="eventName" placeholder="Event Name" name="name">
                  </div>
                </div>
                <div class="form-group">
                  <label for="eventDate" class="col-lg-2 control-label">Event Date</label>
                  <div class="col-lg-10">
                    <input type="text" class="form-control" id="eventDate" placeholder="2015.07.25" name="date">
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-lg-10 col-lg-offset-2">
                    <button type="reset" class="btn btn-default">Cancel</button>
                    <input type="submit" class="btn btn-primary">
                  </div>
                </div>
              </fieldset>
            </form>
          </div>
        </div>
      </div>
    </div>


    <div class="container">
      <footer>
        <div class="row">
          <div class="col-lg-12">

            <ul class="list-unstyled">
              <li class="pull-right"><a href="#top">Back to top</a></li>
              <li><a href="http://news.bootswatch.com" onclick="pageTracker._link(this.href); return false;">Blog</a></li>
              <li><a href="http://feeds.feedburner.com/bootswatch">RSS</a></li>
              <li><a href="https://twitter.com/bootswatch">Twitter</a></li>
              <li><a href="https://github.com/thomaspark/bootswatch/">GitHub</a></li>
              <li><a href="../help/#api">API</a></li>
              <li><a href="../help/#support">Support</a></li>
            </ul>
